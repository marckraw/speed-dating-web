{
  "name": "speed-dating-backend",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "debug-env": "node -e \"console.log('ENV VARS:', Object.keys(process.env).filter(k => k.includes('DATABASE') || k.includes('API') || k.includes('RAILWAY')).map(k => k + '=' + (process.env[k] ? 'SET' : 'UNSET')).join('\\n'))\"",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "check": "npm run type-check && npm run lint",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "eval": "npx tsx evals/run.ts $1",
    "prepare": "relative-deps",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:memory": "vitest run packages/thegrid/src/tests/memory.test.ts",
    "test:qdrant": "npx tsx src/scripts/test-qdrant-memory.ts",
    "test:conversational": "npx tsx src/scripts/test-conversational-memory.ts",
    "figma:help": "npx tsx src/services/AgentsFactories/FigmaToStoryblokAgent/cli-helper.ts help",
    "figma:demo": "npx tsx src/services/AgentsFactories/FigmaToStoryblokAgent/cli-helper.ts demo",
