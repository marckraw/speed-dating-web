    "figma:demo:real-story": "npx tsx src/services/AgentsFactories/FigmaToStoryblokAgent/cli-helper.ts demo real-story",
    "figma:analyze-real": "npx tsx src/services/AgentsFactories/FigmaToStoryblokAgent/cli-helper.ts demo analyze-real",
