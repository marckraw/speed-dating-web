{
  "name": "speed-dating-backend",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "debug-env": "node -e \"console.log('ENV VARS:', Object.keys(process.env).filter(k => k.includes('DATABASE') || k.includes('API') || k.includes('RAILWAY')).map(k => k + '=' + (process.env[k] ? 'SET' : 'UNSET')).join('\\n'))\"",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "check": "npm run type-check && npm run lint",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@hono/node-server": "^1.13.2",
    "@hono/node-ws": "^1.1.7",
    "@hono/zod-validator": "^0.4.1",
    "@types/pg": "^8.11.10",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.1",
    "drizzle-zod": "^0.8.2",
    "elevenlabs": "^1.59.0",
    "fluent-ffmpeg": "^2.1.3",
    "hono": "^4.6.4",
    "hono-pino": "^0.3.0",
    "lowdb": "^7.0.1",
    "openai": "^4.96.0",
    "pg": "^8.13.1",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "uuid": "^9.0.0",
    "ws": "^8.18.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@figma/rest-api-spec": "^0.27.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^20.11.17",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "drizzle-kit": "^0.28.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdom": "^25.0.1",
    "prettier": "^3.2.5",
    "relative-deps": "^1.0.7",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^2.1.5"
  }
}
